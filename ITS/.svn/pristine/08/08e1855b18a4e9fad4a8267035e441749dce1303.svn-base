<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/content_search"
    tools:context="com.itis.activitys.SearchActivity">
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:id="@+id/search_back"
            android:layout_marginTop="@dimen/height_se_20"
            android:layout_marginLeft="@dimen/height_se_20"
            android:layout_marginRight="@dimen/height_se_10"
            android:padding="@dimen/height_se_10"
            android:src="@drawable/search_jiantou"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <EditText
            android:id="@+id/search"
            android:hint="搜索"
            android:textSize="@dimen/text_se_14"
            android:textColor="@color/color_8e8"
            android:padding="@dimen/height_se_10"
            android:background="@drawable/input_search"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/height_se_20"/>
    </LinearLayout>
</RelativeLayout>

<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.itis.activitys.LeaveMessageActivity">
    <RelativeLayout
        android:id="@+id/first"
        android:layout_width="match_parent"
        android:layout_height="@dimen/height_se_50">
        <ImageView
            android:padding="@dimen/height_se_12"
            android:id="@+id/back"
            android:src="@mipmap/nav_button_jiantou"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/text_se_16"
            android:text="@string/message"
            android:padding="@dimen/height_se_12"
            android:layout_centerHorizontal="true"
            android:textColor="@color/color_939"/>
    </RelativeLayout>
    <include
        layout="@layout/item_linear"/>
</LinearLayout>
